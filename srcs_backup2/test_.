#include <vector>
#include <iostream>

int main()
{
    std::vector<int> _more_int;

    _more_int.push_back(10);
    _more_int.push_back(10);
    _more_int.push_back(10);
    _more_int.push_back(10);
    _more_int.push_back(10);
    _more_int.push_back(10);//
    _more_int.push_back(10);//
    _more_int.push_back(10); // 
    _more_int.push_back(10); //
    _more_int.push_back(10); //
    _more_int.push_back(10);
    for (size_t i = 0; i < _more_int.size(); i++)
    {
        std::cout << _more_int[i] << std::endl;
        _more_int.pop_back();
        std::cout << _more_int.size() << std::endl;
    } 
}