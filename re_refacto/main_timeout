#include <ctime>
#include <iostream>



int main()
{
    size_t time_start;
    size_t timeout = 10;

    std::time_t result = std::time(0);

    time_start = std::time(&result);

    std::cout << time_start << std::endl;

  //ÃŸ//
   while ((std::time(&result) - time_start) < timeout)
   {

   }




}